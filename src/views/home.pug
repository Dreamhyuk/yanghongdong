doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Seowon E-Platform
        link(rel="stylesheet", href="/public/css/index.css")
        link(rel="stylesheet", href="https://unpkg.com/mvp.css")
        link(rel='icon', type='image/x-icon', href='assets/favicon.ico')
        script(src='https://use.fontawesome.com/releases/v5.15.3/js/all.js', crossorigin='anonymous')
        link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700', rel='stylesheet', type='text/css')
        link(href='https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic', rel='stylesheet', type='text/css')
    body
    section#page-top
    nav#mainNav.navbar.navbar-expand-lg.bg-secondary.text-uppercase.fixed-top
        .container
            a.navbar-brand(href='#page-top') Seowon University
            button.navbar-toggler.text-uppercase.font-weight-bold.bg-primary.text-white.rounded(type='button', data-bs-toggle='collapse', data-bs-target='#navbarResponsive', aria-controls='navbarResponsive', aria-expanded='false', aria-label='Toggle navigation')
             Menu
            i.fas.fa-bars
            #navbarResponsive.collapse.navbar-collapse
            ul.navbar-nav.ms-auto
                li.nav-item.mx-0.mx-lg-1
                a.nav-link.py-3.px-0.px-lg-3.rounded(href='#portfolio') LOGIN
                li.nav-item.mx-0.mx-lg-1
                a.nav-link.py-3.px-0.px-lg-3.rounded(href='#about') ENTER ROOM
    
    header.masthead.bg-primary.text-white.text-center
        .container.d-flex.align-items-center.flex-column
            img.masthead-avatar.mb-5(src='data:image/jpeg;base64,', alt='...')
            .divider-custom.divider-light
            .divider-custom-line
            .divider-custom-icon
                i.fas.fa-star
            .divider-custom-line
        header
            h1 Seowon E-Platform
        main
        section#about.page-section.bg-primary.text-white.mb-0
            .container
            div#welcome.welcome
                form
                    input(id="nickname" placeholder="nickname", required, type="text")
                    input(id="roomName" placeholder="room name", required, type="text")
                    button Enter Room
                h4 Open Rooms:
                ul
            div#room.room
                h3
                div#myStream.streamWrapper
                    div.streamList
                        video#myCamera(autoplay,playsinline, muted, width="400", height="400")
                        video#peerFace(autoplay,playsinline, width="400", height="400")
                    div.streamBtnList
                        button#mute Mute
                        button#cameraBtn Turn Camera Off
                    select#cameras
                ul.messageList
                form
                    input(placeholder="message", required, type="text")
                    button Send
                    
        script(src="/socket.io/socket.io.js") 
        script(src="/public/js/app.js")
        